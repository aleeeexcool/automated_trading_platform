{
  "name": "automated-trading-platform",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "build:prod": "tsc --project tsconfig.prod.json",
    "run-api": "ts-node src/api",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "test-contracts": "NODE_ENV=test hardhat test --network hardhat",
    "test-contracts-coverage": "hardhat coverage --solcoverjs ./solcover.js",
    "test-integration": "NODE_ENV=test NETWORK=localhost hardhat test --network localhost",
    "test-core": "NODE_ENV=test ts-mocha -p tsconfig.json test-core/*.ts --timeout 60000 --exit",
    "test-core-coverage": "nyc --reporter=lcov --reporter=text-summary ts-mocha -p tsconfig.json test-core/*.ts --timeout 60000 --exit",
    "test-api": "NODE_ENV=test ts-mocha -p tsconfig.json test-api/*.ts --timeout 90000 --exit",
    "deploy-test": "hardhat deploy --network hardhat",
    "deploy-prod-cronos": "hardhat deploy --network cronos",
    "deploy-prod-bsc": "hardhat deploy --network bsc",
    "deploy-prod-mainnet": "hardhat deploy --network mainnet",
    "execute-rollover-cronos": "npx hardhat run scripts/manager/rollover/index.ts --network cronos",
    "execute-rollover-bsc": "npx hardhat run scripts/manager/rollover/index.ts --network bsc",
    "execute-rebalance-v3": "ts-node scripts/manager/rebalance-v3/index.ts",
    "save-uniswap-v3-positions": "ts-node src/actions/saveStrategyUniswapV3Positions.ts",
    "display-swaps-list": "ts-node src/actions/displaySwapsList.ts",
    "display-deposit-tx-list": "ts-node src/actions/displayDepositTxList.ts",
    "display-withdraw-tx-list": "ts-node src/actions/displayWithdrawTxList.ts",
    "display-rebalance-tx-list": "ts-node src/actions/displayRebalanceTxList.ts",
    "display-rebalance-v3-tx-list": "ts-node src/actions/displayUniswapV3Rebalance.ts",
    "display-ability-of-rebalance-v3": "ts-node src/actions/displayAbilityOfUniswapV3Rebalance.ts",
    "display-collect-fees": "ts-node src/actions/displayUniswapV3CollectFees.ts",
    "display-liquidity-amounts": "ts-node src/actions/displayRemoveLiquidityAmountsV3.ts",
    "harvest-tonic": "npx hardhat run scripts/manager/harvest/harvest-tonic.ts --network cronos",
    "harvest-pancake-v3": "ts-node scripts/manager/harvest/harvest-pancake-v3.ts",
    "harvest-pancake-cake-v3": "ts-node scripts/manager/harvest/harvest-pancake-cake-v3.ts",
    "migrate-status": "NODE_ENV=dev sequelize-cli db:migrate:status",
    "migrate-up": "NODE_ENV=dev sequelize-cli db:migrate",
    "migrate-down": "NODE_ENV=dev sequelize-cli db:migrate:undo",
    "test": "hardhat test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basileos/hydropump-contracts.git"
  },
